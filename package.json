{
  "name": "express-template",
  "version": "1.0.0",
  "description": "Node template for an express application",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./build/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "shx rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcarter97/express-template.git"
  },
  "keywords": [],
  "author": "https://github.com/bcarter97",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bcarter97/express-template/issues"
  },
  "homepage": "https://github.com/bcarter97/express-template#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "nodemon": "^2.0.7",
    "shx": "^0.3.3",
    "source-map-support": "^0.5.19"
  }
}
